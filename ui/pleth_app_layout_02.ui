       <string notr="true">/* ---- Low-profile compact filled tabs --------------------------------- */
QTabWidget::pane {
    background: #0f1116;
    border: 1px solid #2b2f3a;
    border-radius: 10px;
    top: 6px;
}

QTabWidget::tab-bar { left: 6px; }

QTabBar { qproperty-drawBase: 0; }

QTabBar::tab {
    background: #141923;
    color: #b6bfda;
    border: 1px solid #2b2f3a;
    border-bottom: none;
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
    padding: 4px 10px;           /* tighter */
    margin-right: 6px;
    min-height: 22px;
    margin-bottom: -1px;         /* sit inside pane border */
    font-size: 12.5px;
}

QTabBar::tab:selected {
    background: #171d29;         /* subtler than before */
    color: #d7dce7;
    border-color: #3e4657;
    border-bottom: 2px solid #5ea1ff; /* thin accent */
}

QTabBar::tab:hover:!selected {
    background: #1a1f2b;
    border-color: #3e4657;
    color: #d7dce7;
}

QTabBar::tab:disabled {
    color: #6c7382;
    background: #141923;
    border-color: #2b2f3a;
}

/* Bottom tabs variant */
QTabBar::tab:bottom {
    border-top-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-left-radius: 6px;
    border-bottom-right-radius: 6px;
    border-top: none;
    border-bottom: 1px solid #2b2f3a;
    margin-top: -1px;
}
QTabBar::tab:bottom:selected {
    border-bottom: 2px solid #5ea1ff;
}
