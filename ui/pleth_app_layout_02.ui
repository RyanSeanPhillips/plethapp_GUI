   <property name="styleSheet">
    <string notr="true">QWidget {
    background-color: #2b2b2b;
    color: #f0f0f0;
}

QLineEdit, QTextEdit, QPlainTextEdit {
    background-color: #3c3c3c;
    color: #ffffff;
    border: 1px solid #5a5a5a;
}

/* Buttons */
QPushButton {
    background-color: #444444;
    color: #ffffff;
    border: 1px solid #5a5a5a;
    padding: 5px;
}
QPushButton:hover { background-color: #555555; }
QPushButton:pressed { background-color: #333333; }

/* Checkboxes */
#centralwidget QCheckBox { color: #ffffff; }
#centralwidget QCheckBox:disabled { color: #7f8899; }

/* Combo boxes — match text boxes */
#centralwidget QComboBox {
    background-color: #3c3c3c;
    color: #ffffff;
    border: 1px solid #5a5a5a;
    padding: 2px 6px;
    border-radius: 3px;
}
#centralwidget QComboBox:hover {
    background-color: #454545;   /* subtle hover */
}
#centralwidget QComboBox:disabled {
    background-color: #2f2f2f;
    color: #9a9a9a;
    border-color: #4a4a4a;
}

/* Drop-down button area (kept slightly lighter so the default arrow is visible) */
#centralwidget QComboBox::drop-down {
    background: #4a4a4a;
    border-left: 1px solid #5a5a5a;
    width: 18px;
}

/* Popup list */
#centralwidget QComboBox QAbstractItemView {
    background: #3c3c3c;
    color: #ffffff;
    border: 1px solid #5a5a5a;
    selection-background-color: #555555;
    selection-color: #ffffff;
    outline: 0;
}

/* If any combo is editable, make its inner editor match too */
#centralwidget QComboBox QLineEdit {
    background-color: #3c3c3c;
    color: #ffffff;
    border: none;
}


/* White outline for checkbox indicator (the little box) */
#centralwidget QCheckBox::indicator {
    width: 16px;
    height: 16px;
    border: 1px solid #ffffff;    /* &lt;- white outline */
    border-radius: 3px;           /* optional */
    background: transparent;      /* keep your dark bg showing through */
    margin-right: 6px;            /* space between box and label */
}

#centralwidget QCheckBox::indicator:hover {
    border-color: #b6bfda;        /* softer hover hint */
}

#centralwidget QCheckBox::indicator:checked {
    background: #0d6efd;          /* blue fill when checked (optional) */
    border-color: #ffffff;        /* keep white outline when checked */
}

#centralwidget QCheckBox::indicator:disabled {
    border-color: #7f8899;        /* muted outline when disabled */
    background: transparent;
}
</string>
