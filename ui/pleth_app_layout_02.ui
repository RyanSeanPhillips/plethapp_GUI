           <string notr="true">/* ---- Compact, flat (underline) tabs — dark theme ------------------- */
QTabWidget::pane {
    background: #0f1116;
    border: 1px solid #2b2f3a;
    border-radius: px;
    top: 1px;               /* tighten gap under the bar */
}

QTabWidget::tab-bar { left: 8px; right: 8px; }

QTabBar {
    qproperty-drawBase: 0;
    qproperty-iconSize: 6px 6px;
}

/* low-profile tabs */
QTabBar::tab {
    background: transparent;        /* no filled pill */
    color: #b6bfda;
    border: none;                   /* no frame */
    padding: 2px 2px;               /* compact height */
    min-height: 12px;
    margin-right: 10px;
    font-size: 10px;
}

/* active = just an accent underline */
QTabBar::tab:selected {
    color: #d7dce7;
    border: none;
    border-bottom: 2px solid #5ea1ff;
    margin-bottom: -2px;            /* sit inside pane border */
}

/* subtle hover */
QTabBar::tab:hover:!selected {
    background: #141923;
    color: #d7dce7;
}

/* disabled */
QTabBar::tab:disabled {
    color: #6c7382;
}

/* bottom tabs: move the underline to the top */
QTabBar::tab:bottom {
    margin-top: -2px;
    margin-bottom: 0;
}
QTabBar::tab:bottom:selected {
    border-top: 2px solid #5ea1ff;
    border-bottom: none;
}
</string>
